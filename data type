
#include<stdio.h>

int main()
{
    int a;
    float b;
    char c;
     printf("enter any character \n");
    scanf("%c",&c);
    printf("the character you have entered is %c \n",c);
    printf("enter the integer\n");
    scanf("%d",&a);
    printf("the integer you have entered is %d \n",a);
    printf("enter the decimal value\n");
    scanf("%f",&b);
    printf("the decimal you have entered is %f \n",b);
    return 0;
}
